#!/usr/bin/expect

set timeout 2

spawn sudo -i -u test /bin/sh -c /snap/bin/test-snapd-tools.sh

expect "bash-4.3$"
send "touch /dev/shm/snap.test-snapd-tools.foo\n"
send "stat /dev/shm/snap.test-snapd-tools.foo\n"

expect {
    "File: '/dev/shm/snap.test-snapd-tools.foo'*" {
        exit 0
    } default {
        exit 1
    }
}
