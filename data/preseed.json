{
    "include": [
        "etc/udev/rules.d",
        "etc/systemd",
        "etc/dbus-1",
        "etc/modprobe.d",
        "etc/modules-load.d",
        "snap",
        "var/lib/extrausers",
        "var/lib/snapd/state.json",
        "var/lib/snapd/apparmor",
        "var/lib/snapd/system-key",
        "var/lib/snapd/assertions",
        "var/lib/snapd/seccomp",
        "var/lib/snapd/cgroup",
        "var/lib/snapd/desktop",
        "var/lib/snapd/sequence",
        "var/lib/snapd/cookie",
        "var/lib/snapd/dbus-1",
        "var/lib/snapd/mount",
        "var/lib/snapd/kernel",
        "var/snap",
        "var/cache/snapd/aux",
        "var/cache/apparmor"
    ],
    "exclude": [
        "var/lib/snapd/snaps/*.snap",
        "var/lib/seed/*"
    ]
}
