#!/bin/sh

set -e

# shellcheck source=snap-wrap/tests/common.sh
. "${srcdir:-.}/snap-wrap/tests/common.sh"

cat >"$TMP/snap.name.app" <<EOF
# some comment
@unrestricted
EOF

printf "Test that the @unrestricted keyword works"
if "$L" snap.name.app /bin/ls / >/dev/null; then
    PASS
else
    FAIL
fi
